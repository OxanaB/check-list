{
    "extends": "../tsconfig.base.json",
    "compileOnSave": true,
    "compilerOptions": {
        "outDir": "./scripts",
        "strict": true,
        "sourceMap": true,
        "noFallthroughCasesInSwitch": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "jsx": "react",
        "lib": [
            "esnext",
            "dom"
        ],
        "target": "es2017",
        "module": "amd",
        "types": [],
    }
}